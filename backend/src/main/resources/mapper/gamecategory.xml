<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssafy.gamecategory">
	<select id="getCategoryList" parameterType="java.lang.Integer"
		resultType="com.ssafy.edu.vue.dto.GameCategory">
		select id, title, description, game_cnt
		from game_category
	</select>
</mapper>